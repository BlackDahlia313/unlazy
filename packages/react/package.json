    "lint:fix": "eslint . --fix",
    "test:types": "tsc --noEmit"
  },
  "dependencies": {
    "unlazy": "workspace:*"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.5.10",
    "@types/node": "^20.12.8",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
