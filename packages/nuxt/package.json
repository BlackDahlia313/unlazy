  "dependencies": {
    "@nuxt/kit": "^3.4.2",
    "defu": "^6.1.2",
    "unlazy": "workspace:*"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.3.0",
    "@nuxt/schema": "^3.4.2",
    "@types/node": "^18.15.12",
