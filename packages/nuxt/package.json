    "@unocss/nuxt": "^0.51.5",
    "@unocss/reset": "^0.51.5",
    "@vue/reactivity": "3.2.47",
    "@vue/runtime-core": "3.2.47",
    "@vue/runtime-dom": "3.2.47",
    "@vue/shared": "3.2.47",
    "nuxt": "^3.4.2",
    "ufo": "1.1.1",
    "unctx": "2.3.0",
    "unstorage": "1.5.0",
    "vue": "3.2.47",
    "vue-router": "4.1.6"
  }
